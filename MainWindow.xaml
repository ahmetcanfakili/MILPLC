<Window x:Class="MILPLC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MILPLC"
        mc:Ignorable="d"
        xmlns:dock="https://github.com/Dirkster99/AvalonDock"
        Title="MILPLC" Height="540" Width="960" WindowStartupLocation="CenterScreen">
    
    <Grid>
        <dock:DockingManager x:Name="dockManager">
            <dock:LayoutRoot>
                <dock:LayoutPanel Orientation="Horizontal">
                    <dock:LayoutDocumentPaneGroup>
                        <dock:LayoutDocumentPane>
                            <dock:LayoutDocument Title="Projects">
                            </dock:LayoutDocument>
                         
                        </dock:LayoutDocumentPane>
                        <dock:LayoutDocumentPane>
                            <dock:LayoutDocument Title="Ladder Editor">
                            </dock:LayoutDocument>
                   
                        </dock:LayoutDocumentPane>
                        <dock:LayoutDocumentPane>
                            <dock:LayoutDocument Title="Components">
                            </dock:LayoutDocument>
                 
                        </dock:LayoutDocumentPane>
                    </dock:LayoutDocumentPaneGroup>
                    
                </dock:LayoutPanel>
            </dock:LayoutRoot>
        </dock:DockingManager>
    </Grid>
    
</Window>
